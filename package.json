{
  "name": "doj-tracker",
  "version": "0.1.0",
  "description": "The Chicago Reporter's tracker of DOJ recommendations for reforms to the Chicago Police Department.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint lib src",
    "sheet2archieml": "./bin/dlsheet --client-secret client_id.json https://docs.google.com/spreadsheets/d/1cj157YGWSDV-c0kBkndZ4XC85tsYSea148mhaP-tpX0/edit | ./bin/sheet2json | ./bin/transformsheetdata | ./bin/json2archieml --prop=recommendations > data.archieml",
    "start": "webpack-dev-server --open",
    "build": "webpack",
    "publish": "npm run build && s3-cli put --acl-public dist/app.bundle.js ${S3_URL}app.bundle.js && cat ./dist/index.html | ./bin/cssselect '#tracker-container' '#tracker-bundle' '#tracker-init' | ./bin/rewritesrc $S3_HTTP_URL | ./bin/wppublish $WP_URL/wp-json"
  },
  "keywords": [
    "CPD",
    "DOJ",
    "police",
    "Chicago",
    "reform"
  ],
  "author": "Geoff Hing <geoffhing@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "archieml": "^0.4.2",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "cheerio": "^1.0.0-rc.2",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "google-auth-library": "^0.10.0",
    "googleapis": "^20.0.1",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "postcss-modules-values": "^1.3.0",
    "react-redux": "^5.0.5",
    "s3-cli": "^0.13.0",
    "slugify": "^1.1.0",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.1.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "^2.7.1",
    "wpapi": "^1.1.2",
    "xlsx": "^0.10.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thechicagoreporter/doj_tracker.git"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "js-search": "^1.4.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.1"
  }
}
